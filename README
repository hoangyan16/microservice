cài mysql docker 
docker run ^
-e MYSQL_ROOT_PASSWORD=password ^
-- name mysql8
-p 3308:3306 ^
-v mysql8-volume:/var/lib/mysql ^
-d mysql:8.0.28

cài rabitmq 
$ docker run -d --hostname my-rabbit --name some-rabbit -e RABBITMQ_DEFAULT_USER=user -e RABBITMQ_DEFAULT_PASS=password rabbitmq:3-management -- cái này tùy
